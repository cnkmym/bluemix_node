{
  "name": "node",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "dependencies": {
    "body-parser": "1.18.2",
    "chai": "4.1.2",
    "chai-spies": "0.7.1",
    "errorhandler": "1.5.0",
    "express": "4.16.2",
    "ip": "1.1.5",
    "istanbul": "0.4.5",
    "mocha": "4.1.0",
    "morgan": "1.9.0",
    "request": "2.83.0"
  },
  "devDependencies": {
    "node-dev": "3.1.3"
  },
  "scripts": {
    "test": "npm run export-unit-result && npm run unit && npm run export-integration-result && npm run integration && npm run coverage-report",
    "unit": "./node_modules/.bin/istanbul cover --dir ./coverage/unit ./node_modules/.bin/_mocha ./test/unit --recursive",
    "integration":"./node_modules/.bin/istanbul cover --dir ./coverage/integration ./node_modules/.bin/_mocha ./test/integration --recursive",
    "export-unit-result": "./node_modules/.bin/_mocha -R json-stream > ./testResult/unit/mochaTestResult.json ./test/unit --recursive ",
    "export-integration-result": "./node_modules/.bin/_mocha -R json-stream >./testResult/integration/mochaTestResult.json ./test/integration --recursive",
    "coverage-report": "./node_modules/.bin/istanbul report",
    "start": "node app/server.js"
  },
